{
  "name": "moulinette-imagesearch",
  "title": "Moulinette Forge Image Search (module)",
  "description": "This module for Moulinette adds capabilities for searching images and generating articles on the fly.",
  "version": "2.2.0",
  "author": "Sven Werlen",
  "esmodules": ["moulinette-imagesearch.js"],
  "styles": ["moulinette-imagesearch.css"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "fr",
      "name": "French",
      "path": "lang/fr.json"
    }
  ],
  "dependencies": [
    {
      "name": "moulinette-core",
      "type": "module"
    }
  ],
  "url": "https://github.com/SvenWerlen/moulinette-imagesearch",
  "download": "https://github.com/SvenWerlen/moulinette-imagesearch/archive/v-2.2.0.zip",
  "manifest": "https://raw.githubusercontent.com/SvenWerlen/moulinette-imagesearch/master/module.json",
  "minimumCoreVersion": "0.7.6",
  "compatibleCoreVersion": "0.8.7"
}
